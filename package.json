{
  "name": "cta-restapi",
  "version": "1.0.0",
  "description": "REST API generic brick for Compass Test Automation",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint --ignore-path .gitignore lib/**/*.js test/**/*.js",
    "test": "mocha -R spec --recursive test",
    "codecoverage": "istanbul cover node_modules/mocha/bin/_mocha -- -R spec && opener ./coverage/lcov-report/index.html",
    "all": "npm run lint && npm run codecoverage",
    "gitlab-ci": "npm all",
    "docs": "jsdoc2md \"lib/**/*.js\" > DOCUMENTATION.md && jsdoc --readme README.md lib -r -d docs && opener ./docs/index.html"
  },
  "engines": {
    "node": "4.2.2"
  },
  "contributors": [
    "Pittawat Mongkonrat <pittawat.mongkonrat@thomsonreuters.com>",
    "Kiettisak Angkanawin <kiettisak.angkanawin@thomsonreuters.com>"
  ],
  "author": "",
  "license": "Apache-2.0",
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.5",
    "bson": "^0.4.23",
    "chai": "^3.4.0",
    "chai-as-promised": "^5.1.0",
    "eslint": "^1.9.0",
    "eslint-config-airbnb": "^1.0.0",
    "eslint-plugin-react": "^3.9.0",
    "istanbul": "^0.4.1",
    "jsdoc": "^3.4.0",
    "jsdoc-to-markdown": "^1.3.6",
    "mocha": "^2.3.4",
    "mock-require": "^1.3.0",
    "nock": "^8.0.0",
    "opener": "^1.4.0",
    "proxyquire": "^1.7.9",
    "require-subvert": "^0.1.0",
    "sinon": "^1.17.0",
    "sinon-as-promised": "^4.0.0"
  },
  "sourceDependencies": {
    "cta-brick": "*",
    "cta-logger": "*",
    "cta-common": "*"
  }
}
